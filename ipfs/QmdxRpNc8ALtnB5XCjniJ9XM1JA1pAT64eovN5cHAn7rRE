pragma solidity ^0.7.4;
contract Demo3{
    function test1GetLength() public pure returns(uint256){
        uint[10] memory bid;
        return bid.length;
    }
    function test2GetLength() public pure returns(uint256){
        uint[] memory bid = new uint[](20);
        return bid.length;
    }

}